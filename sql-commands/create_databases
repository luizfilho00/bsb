CREATE TABLE `conta` (
	`id`	INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
	`conta`	TEXT NOT NULL,
	`digito` TEXT NOT NULL,
	`senha`	TEXT NOT NULL
);

CREATE TABLE `cliente` (
	`id`	INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
	`nome`	TEXT NOT NULL,
	`apelido`	TEXT NOT NULL,
	`senha`	TEXT NOT NULL,
	`conta`	INTEGER,
	foreign key(conta) references conta(id)
);

CREATE TABLE gerente(
	id INTEGER NOT NULL PRIMARY KEY autoincrement,
	usuario TEXT NOT NULL,
	senha TEXT NOT NULL
);